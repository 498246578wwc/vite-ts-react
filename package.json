{
  "name": "vite-react-universal-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode production",
    "build:test": "tsc && vite build --mode test",
    "build:docker": "tsc && vite build --mode docker",
    "preview": "vite preview",
    "lint": "tsc && eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"./**/*.{html,tsx,ts,js,json,md}\"",
    "prepare": "husky",
    "docker:build": "tsc && docker build -t docker-react .",
    "docker:run": "docker rm -f docker-react && docker run --name docker-react -d -p 80:8082 docker-react",
    "release:local": "pnpm docker:build && pnpm docker:run",
    "log:tree": "tree -N -I \"node_modules|.git|.idea\" -a"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.0",
    "@react-spring/web": "^9.7.5",
    "@react-three/drei": "^9.121.4",
    "@react-three/fiber": "^8.17.14",
    "@tailwindcss/vite": "^4.0.0",
    "@types/lodash": "^4.17.14",
    "@types/qs": "^6.9.18",
    "@use-gesture/react": "^10.3.1",
    "antd": "^5.23.2",
    "axios": "^1.7.9",
    "classnames": "^2.5.1",
    "less": "^4.2.2",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.5",
    "react-router-dom": "^7.1.5",
    "react-spring": "^9.7.5",
    "tailwind-scrollbar-hide": "^2.0.0",
    "tailwindcss": "^4.0.0",
    "three": "^0.173.0",
    "vconsole": "^3.15.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.173.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "eslint": "^8.54.0",
    "eslint-config-code-style-lint-react-ts": "^3.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "stylelint": "^16.11.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "typescript": "^5.7.2",
    "unplugin-auto-import": "^0.18.6",
    "vite": "^6.0.2"
  }
}
